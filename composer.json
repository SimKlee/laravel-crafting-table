{
    "name": "simklee/laravel-crafting-table",
    "description": "Laravel Package to craft extended models.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SimKlee\\LaravelCraftingTable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimKlee\\LaravelCraftingTable\\Tests\\": "tests",
            "SimKlee\\LaravelCraftingTable\\Tests\\Unit\\": "tests/Unit"
        }
    },
    "authors": [
        {
            "name": "Simon Kleeschulte",
            "email": "simon.kleeschulte@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "squizlabs/php_codesniffer": "4.0.x-dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "SimKlee\\LaravelCraftingTable\\LaravelCraftingTableServiceProvider"
            ]
        }
    },
    "scripts": {
        "codestyle-check": "./vendor/bin/phpcs src/ tests/",
        "codestyle-fix": "./vendor/bin/phpcbf src/ tests/"
    }
}
